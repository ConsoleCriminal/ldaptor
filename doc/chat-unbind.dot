digraph chat_unbind {
  ranksep=0.1;

  subgraph start {
    rank=same;

    cStart [label="Client"];
    sStart [label="Server"];
  };

  subgraph cluster_client {
    style=invis;
    node [width=0, height=0, label=""];
    edge [weight=100, arrowhead=none];

    cEnd [style=invis];
    cStart -> c1 -> c2 -> c3 -> c4 -> c5;
    c5 -> cEnd [arrowhead=normal];
  };

  subgraph cluster_server {
    style=invis;

    node [width=0, height=0, label=""];
    edge [weight=100, arrowhead=none];

    sEnd [style=invis];
    sStart -> s1 -> s2 -> s3 -> s4 -> s5 -> s6;
    s6 -> sEnd [arrowhead=normal];
  };

  c1 -> s2 [label="unbind"];
  s3 -> c4 [label="unbind ok"];
  c5 -> s6 [label="disconnect"];
};
